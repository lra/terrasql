{
  "name": "terrasql",
  "version": "0.0.2",
  "description": "Diff tool for SQL tables, like terraform.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf node_modules/ build/",
    "build": "tsc",
    "markdownlint": "markdownlint --ignore node_modules/ **/*.md",
    "tslint": "tslint src/**/*.ts",
    "circleci-markdownlint": "circleci local execute --job=markdownlint",
    "circleci-tslint": "circleci local execute --job=tslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lra/terrasql.git"
  },
  "keywords": [
    "sql",
    "ddl",
    "terraform",
    "diff",
    "gitops",
    "mysql",
    "postgresql"
  ],
  "author": "Laurent Raufaste <analogue@glop.org> (http://www.glop.org/)",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/lra/terrasql/issues"
  },
  "homepage": "https://github.com/lra/terrasql#readme",
  "bin": {
    "terrasql": "./build/index.js"
  },
  "dependencies": {
    "docopt": "^0.6.2",
    "mysql": "^2.16.0",
    "pjson": "^1.0.9"
  },
  "devDependencies": {
    "@types/mysql": "^2.15.5",
    "@types/docopt": "^0.6.31",
    "markdownlint-cli": "^0.13.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "files": [
    "build/**/*.js"
  ]
}
